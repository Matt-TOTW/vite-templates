{
  "name": "vite-templates",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "turbo build",
    "lint": "turbo lint",
    "clean": "turbo clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo build --filter=docs^... && changeset publish"
  },
  "private": true,
  "devDependencies": {
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "@changesets/cli": "^2.22.0",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "turbo": "^1.9.3"
  },
  "prettier": {
    "singleQuote": true,
    "useTabs": false
  },
  "workspaces": [
    "packages/*"
  ]
}